   ],
   "source": [
    "# state vector for a 3-body system\n",
    "\n",
    "n = 3\n",
    "bond_dim = 3\n",
    "\n",
    "psi = np.random.rand(*[2]*n)\n",
    "psi /= np.linalg.norm(psi)\n",
    "\n",
    "# create MPS\n",
    "Ms, Ss = mps.dense_to_mps(psi, bond_dim, n)\n",
    "\n",
    "[M.shape for M in Ms]"
   ]
